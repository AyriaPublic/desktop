{
  "name": "ayria-desktop",
  "version": "0.0.0",
  "description": "GUI Electron app for the Ayria Network Platform.",
  "keywords": [
    "Ayria",
    "Ayria Desktop",
    "Electron"
  ],
  "homepage": "https://github.com/AyriaPublic/ayria-desktop",
  "bugs": "https://github.com/AyriaPublic/ayria-desktop/issues",
  "license": "GPL-3.0",
  "contributors": [
    "Selwyn <talk@selwyn.cc> (https://selwyn.cc/)",
    "Will Tange <will@brt.sexy> (https://brt.sexy)"
  ],
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AyriaPublic/ayria-desktop.git"
  },
  "scripts": {
    "postinstall": "electron-rebuild",
    "start": "electron .",
    "test": "ava test",
    "preversion": "npm test",
    "postversion": "git push",
    "lint": "eslint src/*.js src/**/*.js test/*.js",
    "pack": "run-s pack:*",
    "pack:linux": "electron-packager . --platform=linux --arch=x64",
    "pack:windows": "electron-packager . --platform=win32 --arch=x64",
    "ci": "run-p test 'lint -- --max-warnings 0'"
  },
  "dependencies": {
    "concat-stream": "^1.6.0",
    "env-paths": "^1.0.0",
    "flat-cache": "^1.2.2",
    "github-slugid": "^1.0.1",
    "hyperquest": "^2.1.2",
    "mkdirp": "^0.5.1",
    "object.entries": "^1.0.4",
    "pify": "^2.3.0",
    "pouchdb": "^6.2.0",
    "pouchdb-upsert": "^2.2.0",
    "ramda": "^0.24.1",
    "vdfjs": "^2.0.3",
    "winreg": "^1.2.2",
    "yauzl": "^2.8.0",
    "yazl": "^2.4.2"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "electron": "~1.7.4",
    "electron-packager": "^8.4.0",
    "electron-rebuild": "^1.5.7",
    "electron-reload": "^1.1.0",
    "eslint": "^4.1.1",
    "npm-run-all": "^4.0.0",
    "semver-level-harmony": "^1.0.0",
    "spectron": "~3.7.2"
  }
}
